SELECT p.product_id,'<?xml version="1.0" encoding="ISO-8859-1"?><P_SERVICIOS_ENERGETICOS_100316 ID_TIPOPAQUETE="100316"><CONTRACT>'||P.SUBSCRIPTION_ID||'</CONTRACT>
<RECEPTION_TYPE_ID>'||&MEDIO_RECEPCION||'</RECEPTION_TYPE_ID>
<COMMENT_>'||&COMENTARIO||'</COMMENT_><ADDRESS_ID>'||DI.ADDRESS_ID||'</ADDRESS_ID><CONTACT_ID>'||SUSCCLIE||'</CONTACT_ID>
<CATEGORIA>'||se.category_||'</CATEGORIA><SUBCATEGORIA>'||se.subcategory_||'</SUBCATEGORIA><M_INSTALACION_DE_SERVICIOS_ENERGETICOS_100316>
<COMMERCIAL_PLAN_ID>'||DECODE(se.category_,1,59, 60)||'</COMMERCIAL_PLAN_ID><C_ENERGIA_SOLAR_10349/></M_INSTALACION_DE_SERVICIOS_ENERGETICOS_100316></P_SERVICIOS_ENERGETICOS_100316>'
FROM OPEN.PR_PRODUCT P, OPEN.GE_GEOGRA_LOCATION L, OPEN.AB_ADDRESS DI, SUSCRIPC, OPEN.AB_SEGMENTS SE 
WHERE SUSCIDDI=DI.ADDRESS_ID
  AND L.GEOGRAP_LOCATION_ID=DI.GEOGRAP_LOCATION_ID
--  AND SUBSCRIPTION_ID=1999679
  AND SUSCCICL=301
  AND PRODUCT_TYPE_ID=7014
  AND SUSCCODI=P.SUBSCRIPTION_ID
  AND SE.SEGMENTS_ID=DI.SEGMENT_ID ;
  
  
SELECT '<?xml version="1.0" encoding="ISO-8859-1"?><P_SERVICIOS_ENERGETICOS_100316 ID_TIPOPAQUETE="100316"><CONTRACT>'||susccodi||'</CONTRACT>
<RECEPTION_TYPE_ID>'||&MEDIO_RECEPCION||'</RECEPTION_TYPE_ID>
<COMMENT_>'||&COMENTARIO||'</COMMENT_><ADDRESS_ID>'||di.ADDRESS_ID||'</ADDRESS_ID><CONTACT_ID>'||SUSCCLIE||'</CONTACT_ID>
<CATEGORIA>'||se.category_||'</CATEGORIA><SUBCATEGORIA>'||se.subcategory_||'</SUBCATEGORIA><M_INSTALACION_DE_SERVICIOS_ENERGETICOS_100316>
<COMMERCIAL_PLAN_ID>'||DECODE(se.category_,1,59, 60)||'</COMMERCIAL_PLAN_ID><C_ENERGIA_SOLAR_10349/></M_INSTALACION_DE_SERVICIOS_ENERGETICOS_100316></P_SERVICIOS_ENERGETICOS_100316>'
FROM OPEN.GE_GEOGRA_LOCATION L, OPEN.AB_ADDRESS DI, SUSCRIPC S, OPEN.AB_SEGMENTS SE 
WHERE S.SUSCIDDI=DI.ADDRESS_ID
  AND L.GEOGRAP_LOCATION_ID=DI.GEOGRAP_LOCATION_ID
  AND susccodi=67397515
  AND SUSCCICL=301
  AND SE.SEGMENTS_ID=DI.SEGMENT_ID  
  and not exists(select null from open.pr_product p where p.subscription_id=susccodi and p.product_type_id=7057)
--67397515
