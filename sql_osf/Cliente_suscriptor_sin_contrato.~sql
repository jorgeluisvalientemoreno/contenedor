---cliento sin producto
select gs.subscriber_id,
       gs.parent_subscriber_id,
       gs.ident_type_id,
       gs.subscriber_type_id,
       gs.identification,
       gs.subscriber_name,
       gs.subs_last_name,
       gs.e_mail,
       gs.url,
       gs.address_id,
       gs.address,
       aa.address,
       aa.geograp_location_id,
       gs.phone,
       gs.active,
       gs.economic_activity_id,
       gs.exoneration_document,
       gs.subs_status_id,
       gs.marketing_segment_id,
       gs.contact_id,
       gs.country_id,
       gs.data_send,
       gs.vinculate_date,
       gs.subs_second_last_name,
       gs.accept_call,
       gs.taxpayer_type,
       gs.authorization_type,
       gs.collect_program_id,
       gs.collect_entity_id,
       gs.category_id,
       gs.doc_date_of_issue,
       gs.doc_place_of_issue,
       gs.is_corporative,
       aa.address_id,
       aa.process_number,
       aa.segment_id,
       aa.way_id,
       aa.way_type,
       aa.way_number,
       aa.way_letter,
       aa.way_geo_sector,
       aa.cross_way_id,
       aa.cross_way_type,
       aa.cross_way_number,
       aa.cross_way_letter,
       aa.cross_way_geo_sect,
       aa.is_urban,
       aa.house_number,
       aa.house_letter,
       aa.address_complement,
       aa.address_parsed,
       aa.installed_lines,
       aa.transact_lines,
       aa.installed_air_lines,
       aa.transact_air_lines,
       aa.estate_number,
       aa.is_main,
       aa.active,
       aa.description,
       aa.zip_code_id,
       aa.neighborthood_id,
       aa.cadastral_id,
       aa.geocode_id,
       aa.grammar_id,
       aa.extended_zip,
       aa.shape,
       aa.father_address_id,
       aa.previous_value,
       aa.is_valid,
       aa.verified
  from open.GE_SUBSCRIBER gs
 inner join OPEN.AB_ADDRESS aa
    on aa.address_id = gs.address_id
   and aa.address like 'C%%'
 where 1 = 1
   and (select count(1)
          from open.suscripc s
         where gs.subscriber_id = s.suscclie) = 0
   and (select count(1)
          from open.pr_product pp
         where pp.address_id = gs.address_id) = 0
   and gs.vinculate_date > sysdate - 350;
