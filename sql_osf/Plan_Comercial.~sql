select * from open.CC_COMMERCIAL_PLAN WHERE COMMERCIAL_PLAN_ID = 4;

select * from OPEN.CC_COM_SEG_PLAN WHERE COMMERCIAL_PLAN_ID = 4;
select * frOM OPEN.CC_PLAN_CLASEOFERTA WHERE PLAN_COMERCIAL_ID = 4;

SELECT *
  FROM OPEN.LDC_TIPO_TRAB_PLAN_CCIAL
--WHERE TT_PLAN_CCIAL_PLAN_ID=4;
 WHERE TT_PLAN_CCIAL_ACTIVIDAD IN (4000036, 4295276);

SELECT *
  FROM OPEN.COMPSESU
 WHERE CMSSPLCO = 4
   AND CMSSSESU = 17100377;
SELECT * FROM OPEN.MO_BILL_DATA_CHANGE;

SELECT COMMERCIAL_PLAN_ID FROM OPEN.PR_PRODUCT WHERE PRODUCT_ID = 17100377;

SELECT COMMERCIAL_PLAN_ID, PRODUCT_ID
  FROM OPEN.MO_MOTIVE
 WHERE PACKAGE_ID = 12074171;

SELECT * FROM OPEN.MO_COMPONENT WHERE PACKAGE_ID = 12074171;
SELECT * FROM OPEN.MO_PACKAGE_PAYMENT WHERE PACKAGE_ID = 12074171;
SELECT * FROM OPEN.MO_BILL_DATA_CHANGE WHERE PACKAGE_ID = 12074171;
select *
  from open.or_order_activity a, open.or_task_type t
 where package_id = 12074171
   and t.task_type_id = a.task_type_id
   and a.status = 'F'
   AND CONCEPT = 193;
SELECT * FROM OPEN.OR_TASK_TYPE;

SELECT * -- A.COMMERCIAL_PLAN_ID ID, A.DESCRIPTION
  FROM CC_COMMERCIAL_PLAN A
 WHERE A.INITIAL_DATE <= SYSDATE
   and A.FINAL_DATE >= SYSDATE
   and A.PACKAGE_TYPE_ID = 587
   and A.PRODUCT_TYPE_ID = 7014 A.COMMERCIAL_PLAN_ID = :ID
 A.DESCRIPTION like :DESCRIPTION
